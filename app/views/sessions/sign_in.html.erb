<section>
   <h1>Sign in</h1>
   <%= form_tag '/sign-in' do %>
      <%= label_tag :username%>
      <%= text_field_tag :username, params[:username], :required => true %>
      <%= label_tag :password%>
      <%= password_field_tag :password, params[:password], :required => true %>
      <%= submit_tag "Sign in"%>
      <%= render partial: 'partials/flashes' %>
   <%end%>
   <p>Don't have an account?</p>
   <%= button_to "Sign up", '/sign-up', method: :get %>
</section>

